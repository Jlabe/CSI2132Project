    <servlet-name>UpdateCustomerServlet</servlet-name>
    <servlet-class>com.demo.UpdateCustomerServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>EditHotelServlet</servlet-name>
    <servlet-class>com.demo.EditHotelServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>UpdateHotelServlet</servlet-name>
    <servlet-class>com.demo.UpdateHotelServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SearchRoomsServlet</servlet-name>
    <url-pattern>/searchRooms</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>BookingServlet</servlet-name>
    <url-pattern>/bookingServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>RentingServlet</servlet-name>
    <url-pattern>/RentingServlet</url-pattern>
  </servlet-mapping>

