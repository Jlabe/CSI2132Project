  <servlet>
    <servlet-name>DeleteEmployeeServlet</servlet-name>
    <servlet-class>com.demo.DeleteEmployeeServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>DeleteHotelServlet</servlet-name>
    <servlet-class>com.demo.DeleteHotelServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>DeleteCustomerServlet</servlet-name>
